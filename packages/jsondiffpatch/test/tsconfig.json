{
	"compilerOptions": {
		"target": "es6",
		"module": "node16",
		"types": ["node"],
		"declaration": true,
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitAny": true
	}
}
